-- ===== SERVICES =====
local MarketplaceService = game:GetService("MarketplaceService")
local placeId = game.PlaceId

local mapName = "UNKNOWN"
pcall(function()
    mapName = MarketplaceService:GetProductInfo(placeId).Name
end)
mapName = string.upper(mapName)

