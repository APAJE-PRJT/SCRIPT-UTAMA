-- ===== TOTAL =====
local total = 0
for _ in pairs(Mountains) do total += 1 end

Tab:CreateParagraph({
    Title = "üìä TOTAL GUNUNG",
    Content = tostring(total)
})

-- ===== BUTTONS (BUAT SEKALI) =====
local Buttons = {}

local function ExecuteMountain(name, data)
    if not data.script or data.script == "" then
        Rayfield:Notify({
            Title = "‚ö†Ô∏è Script Kosong",
            Content = name,
            Duration = 3
        })
        return
    end
    loadstring(game:HttpGet(data.script))()
end

for name, data in pairs(Mountains) do
    Buttons[name] = Tab:CreateButton({
        Name = name,
        Callback = function()
            ExecuteMountain(name, data)
        end
    })
end

-- ===== AUTO DETECT + ‚≠ê HIGHLIGHT =====
for name, data in pairs(Mountains) do
    if (data.placeId ~= 0 and data.placeId == placeId) or mapName:find(name) then
        Rayfield:Notify({
            Title = "‚≠ê Map Detected",
            Content = name,
            Duration = 5
        })
        if Buttons[name] then
            Buttons[name]:SetName("‚≠ê "..name)
        end
        break
    end
end
